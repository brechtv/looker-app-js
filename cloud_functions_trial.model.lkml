connection: "brecht_bq_playground"

explore: test_cloud_functions {}

view: test_cloud_functions {
  derived_table: {
    sql:
      SELECT
        1 AS id, 'Bulgaria' AS country
      UNION ALL
      SELECT
        2 AS id, 'China' AS country
      UNION ALL
      SELECT
        3 AS id, 'Canada' AS country
      UNION ALL
      SELECT
        4 AS id, 'Burkina Faso' AS country
      UNION ALL
      SELECT
        5 AS id, 'China' AS country

      ;;
  }

  dimension: id {
    type: number
    sql: ${TABLE}.id ;;
  }

  dimension: country {
    type: string
    sql: ${TABLE}.country ;;
    action: {
      label: "Geocode!"
      url: "https://us-central1-brechtv-playground.cloudfunctions.net/geocodeAddress"
      icon_url: "data:image/png;base64,"
    param: {
      name: "address"
      value: "{{ value }}"
    }
  }
  }


}
